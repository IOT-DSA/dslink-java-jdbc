{
  "name": "dslink-java-jdbc",
  "version": "0.0.2",
  "description": "Java JDBC DSLink",
  "license": "Apache",
  "author": {
    "name": "Petr Shvets",
    "email": "p.shvets@dglogik.com"
  },
  "main": "bin/dslink-java-jdbc",
  "repository": {
    "type": "git",
    "url": "https://github.com/IOT-DSA/dslink-java-jdbc"
  },
  "bugs": {
    "url": "https://github.com/IOT-DSA/dslink-java-jdbc/issues"
  },
  "configs": {
    "name": {
      "type": "string",
      "default": "jdbc"
    },
    "broker": {
      "type": "url"
    },
    "token": {
      "type": "string"
    },
    "nodes": {
      "type": "path",
      "default": ".nodes.json"
    },
    "key": {
      "type": "path",
      "default": ".key"
    },
    "log": {
      "type": "enum",
      "default": "info"
    },
    "handler_class": {
      "type": "string",
      "default": "org.dsa.iot.jdbc.JdbcDslink"
    }
  }
}
